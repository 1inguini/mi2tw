{
	"compilerOptions": {
		"noEmit": true,
		"module": "esnext",
		"target": "esnext",
		"lib": [
			"esnext"
		],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"strict": true,
		"moduleResolution": "node",
		"types": [
			"@cloudflare/workers-types"
		]
	},
	"ts-node": {
		"compilerOptions": {
			"module": "CommonJS"
		}
	},
	"exclude": [
		"node_modules",
		"dist",
		"test"
	]
}